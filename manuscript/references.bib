@article{mehta_putting_2000,
	title = {Putting the individual back into individual growth curves},
	volume = {5},
	issn = {1082-989X},
	url = {https://auth.lib.unc.edu/ezproxy_auth.php?url=https://search.ebscohost.com/login.aspx?direct=true&db=pdh&AN=2000-03131-002&site=ehost-live&scope=site},
