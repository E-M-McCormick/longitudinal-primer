@article{verachtert_season_2010,
	title = {Season of birth and school success in the early years of primary education},
	volume = {36},
	issn = {0305-4985},
	url = {http://www.jstor.org/stable/25699586},
	abstract = {Several studies have reported significant relationships between children's season of birth and measures of their academic success (i.e., the 'season of birth effect'). Whereas most of these studies were cross-sectional, the current study uses growth curve modelling to analyse longitudinal data on 3,187 children in Flemish primary education. The results indicate season of birth effects on both grade retention and mathematics achievement during the first two years of primary school. Because the Flemish cut-off date is 31 December, children born in the fourth quarter (October-November-December) invariably are among the youngest in their grade age group. Almost 20\% of these children were found to have been retained or referred to special education by the end of Grade 2, whereas for children born in the first quarter (January-February-March), this was only 6.34\%. First quarter-born children also showed moderately higher mathematics achievement at the start of first grade. During the next two school years, this achievement gap between children born in the first and the fourth quarter narrowed significantly. Finally, differentiated instruction was not found to be related to the decrease of the season of birth effect.},
	pages = {285--306},
	number = {3},
	journaltitle = {Oxford Review of Education},
